<templateSet group="html/xml">
  <template name="src1" value="&lt;script type=&quot;text/javascript&quot;&gt;&#10;  $statement$&#10;&lt;/script&gt;" description="inner javascript" toReformat="false" toShortenFQNames="true">
    <variable name="statement" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="src2" value="&lt;script type=&quot;text/javascript&quot; src=&quot;$src$&quot;&gt;&lt;/script&gt;" description="outer javascript file" toReformat="false" toShortenFQNames="true">
    <variable name="src" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="CD" value="&lt;![CDATA[&#10;$SELECTION$&#10;]]&gt;" description="CDATA" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="MXML" value="true" />
      <option name="XML" value="true" />
      <option name="XSL_TEXT" value="true" />
    </context>
  </template>
</templateSet>